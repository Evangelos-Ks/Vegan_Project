@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<div id="pageheader" class="docsection-wrapper">
    <div class="docsection boxed">
        <div class="breadcrumbs-wrapper clearfix">
            <div class="breadcrumbs">
                <section>
                    <p><a href="/">Home</a> > <a href="#">Error</a></p>
                </section>
            </div>
        </div>
        <div class="title-wrapper clearfix">
            <div class="title">
                <div>
                    <h1>Error</h1>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="" class="docsection-wrapper">
    <div class="docsection boxed">
        <section id="message">
            <div class="dottedbox">
                <form action="~/Home/Index">
                    <div class="notice">An error occurred while processing your request.</div>
                    <button type="submit" value="" class="btn">Click here to continue</button>
                </form>
            </div>
        </section>
    </div>
</div>
