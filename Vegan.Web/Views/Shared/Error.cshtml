@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<link href="~/Content/VeganCSS/EmptyViewCart.css" rel="stylesheet" />

<div class="contentbox-wrapper">
    <div class="contentbox">
        <div id="pageheader" class="docsection-wrapper">
            <div class="docsection boxed">
                <div class="breadcrumbs-wrapper clearfix">
                    <div class="breadcrumbs">
                        <section>
                            <p><a href="/Home/Index">Home</a></p>
                        </section>
                    </div>
                </div>
                <div class="title-wrapper clearfix">
                    <div class="title">
                        <header>
                            <h1>Error</h1>
                        </header>
                    </div>
                </div>
            </div>
        </div>
        <div id="" class="docsection-wrapper">
            <div class="docsection boxed">
                <section id="message">
                    <div class="dottedbox">
                        <div class="notice">An error occurred while processing your request.</div>
                        <a type="button" class="btn" href='/Home/Index'>Click here to continue</a>
                    </div>
                </section>
            </div>
        </div>
    </div>
</div>


