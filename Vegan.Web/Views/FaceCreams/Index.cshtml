<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImageURL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Directions)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameOfIngredient)
        </th>
        @{
            bool includedValuOfIngredient = false;
            foreach (var item in Model)
            {
                if (string.IsNullOrWhiteSpace(item.ValueOfIngredient))
                {
                    includedValuOfIngredient = false;

                }
                else
                {
                    includedValuOfIngredient = true;

                }


            }

            if (includedValuOfIngredient)
            {
                <th>
                    @Html.DisplayNameFor(model => model.ValueOfIngredient)
                </th>
            }
        }
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                <img src="@item.ImageURL" width='100' height='100' alt="@item.Title" />
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Directions)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NameOfIngredient)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValueOfIngredient)
            </td>

            <td>
                @Html.ActionLink("Edit", "EditFaceCream", new { productId = item.Id }) |
                @Html.ActionLink("Details", "DetailsFaceCream", new { productId = item.Id }) |
                @Html.ActionLink("Delete", "DeleteFaceCream", new { productId = item.Id })
            </td>
        </tr>
    }

</table>
